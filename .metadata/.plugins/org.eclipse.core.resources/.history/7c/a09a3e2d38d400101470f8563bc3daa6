package com.project.mediverse.repository;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.project.mediverse.entity.Prescription;

import java.util.List;

@Repository
public interface PrescriptionRepository extends JpaRepository<Prescription, Long> {
    // Custom queries, for example:
    List<Prescription> findByCustomerId(Long customerId);
    List<Prescription> findByMedicineId(Long medicineId);
}
